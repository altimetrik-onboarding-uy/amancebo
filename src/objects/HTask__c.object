    <fields>
        <fullName>Points__c</fullName>
        <description>Calculates the task points based in the type and difficulty of the Task</description>
        <externalId>false</externalId>
        <formula>IF( TEXT(Type__c) = &apos;TO-DO&apos;, 
  IF( TEXT(Difficulty__c) = &apos;Trivial&apos;, 1, 
  IF( TEXT(Difficulty__c) = &apos;Easy&apos;, 2,
  IF( TEXT(Difficulty__c) = &apos;Normal&apos;, 3,
  IF( TEXT(Difficulty__c) = &apos;Hard&apos;, 5, 0
  )))),
IF( TEXT(Type__c) = &apos;Daily&apos;,
  IF( TEXT(Difficulty__c) = &apos;Trivial&apos;, 0.5, 
  IF( TEXT(Difficulty__c) = &apos;Easy&apos;, 1,
  IF( TEXT(Difficulty__c) = &apos;Normal&apos;, 1.5,
  IF( TEXT(Difficulty__c) = &apos;Hard&apos;, 2.5, 0
  )))),
IF( TEXT(Type__c) = &apos;Habit&apos;, 1, 0)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Points</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
